{
  "name": "comunica.italia.it",
  "description": "xxx",
  "type": "project",
  "authors": [
    {
      "name": "wellnet",
      "email": "info@wellnet.it"
    }
  ],
  "require": {
    "boedah/robo-drush": "3.1.1",
    "composer/installers": "1.5.0",
    "consolidation/robo": "1.3.0",
    "cweagans/composer-patches": "1.6.4",
    "drupal/config_split": "1.3",
    "drupal/console": "~1.0",
    "drupal/core": "8.5.4",
    "drupal/eu_cookie_compliance": "1.0",
    "drupal/menus_attribute": "1.1",
    "drupal/metatag": "1.4",
    "drupal/monolog": "1.0",
    "drupal/login_security": "1.5",
    "drupal/redis": "1.0",
    "drupal/webform": "5.0-rc14",
    "drush/drush": "9.3.0",
    "wikimedia/composer-merge-plugin": "v1.4.1",
    "drupal/field_group": "^1.0",
    "drupal/ds": "^3.1",
    "drupal/bg_image_formatter": "^1.2",
    "drupal/default_content_moderation": "1.0-beta2"
  },
  "require-dev": {
    "components/highlightjs": "9.7.0",
    "dealerdirect/phpcodesniffer-composer-installer": "v0.4.4",
    "drupal/coder": "2.12",
    "drupal/config_devel": "1.2",
    "drupal/config_inspector": "1.0-beta1",
    "drupal/default_content": "1.0-alpha7",
    "drupal/default_content_deploy": "1.0-alpha5",
    "drupal/devel": "1.2",
    "edgedesign/phpqa": "v1.18.0",
    "d3/d3": "3.5.17",
    "squizlabs/php_codesniffer": "^3.1"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "autoloader-suffix": "Drupal8"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "d3/d3",
        "version": "v3.5.17",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/d3/d3/archive/v3.5.17.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "components/highlightjs",
        "version": "9.7.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
          "type": "zip"
        }
      }
    }
  ],
  "extra": {
    "merge-plugin": {
      "include": [],
      "recurse": false,
      "replace": false,
      "merge-extra": false
    },
    "patches": {
      "monolog/monolog": {
        "Set a limit to normalizer depth": "patches/monolog-fix-normalizer-depth.patch"
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    }
  }
}
